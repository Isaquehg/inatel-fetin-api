				{
					"name": "Login Admin",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Log the raw response text",
									"const responseText = pm.response.text();",
									"console.log('Raw response text:', responseText);",
									"",
									"// Check if the response is empty",
									"if (responseText) {",
									"    try {",
									"        // Parse the response as JSON",
									"        const responseJson = pm.response.json();",
									"        console.log('Response JSON:', responseJson);",
									"",
									"        // Check for the JWT token in the expected key",
									"        const jwt = responseJson.Bearer || responseJson.token; // Adjust if the token is under a different key",
									"        if (jwt) {",
									"            // Set the JWT token in globals",
									"            pm.globals.set('jwt_admin', `${jwt}`);",
									"            console.log('JWT set as jwt_admin:', jwt);",
									"        } else {",
									"            console.error('JWT not found in response JSON:', responseJson);",
									"        }",
									"    } catch (e) {",
									"        // Log parsing errors",
									"        console.error('Error parsing response JSON:', e.message);",
									"    }",
									"} else {",
									"    console.error('Received an empty response.');",
									"}",
									"",
									"// Check and log the Content-Type of the response",
									"const contentType = pm.response.headers.get('Content-Type');",
									"console.log('Response Content-Type:', contentType);",
									"if (!contentType.includes('application/json')) {",
									"    console.error('Unexpected Content-Type:', contentType);",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
